{
    "name": "@doki-land/live2d",
    "version": "0.0.0",
    "keywords": [
        "live2d",
        "pixi",
        "typescript"
    ],
    "description": "Live2D TypeScript runtime based on pixi-live2d-display",
    "author": "Aster <192608617@qq.com>",
    "license": "LGPL-3.0-or-later",
    "type": "module",
    "main": "dist/l2d.umd.js",
    "module": "dist/l2d.esm.js",
    "types": "src/index.ts",
    "exports": {
        ".": {
            "require": "./dist/l2d.umd.js",
            "import": "./dist/l2d.esm.js",
            "types": "./src/index.ts"
        }
    },
    "files": [
        "dist",
        "lib",
        "src"
    ],
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview"
    },
    "dependencies": {
        "@pixi/interaction": "6.*",
        "@pixi/ticker": "6.*",
        "pixi-live2d-display": "^0.4.0",
        "pixi.js": "6.*"
    }
}
