{
    "name": "@doki-land/live2d",
    "version": "0.0.10",
    "keywords": [
        "live2d",
        "pixi",
        "typescript"
    ],
    "description": "Live2D TypeScript runtime based on pixi",
    "author": "Aster <192608617@qq.com>",
    "license": "LGPL-3.0-or-later",
    "type": "module",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "import": "./src/index.ts",
            "require": "./dist/l2d.umd.js"
        },
        "./*": {
            "types": "./src/*/index.ts",
            "import": "./src/*/index.ts"
        }
    },
    "files": [
        "dist",
        "lib",
        "src"
    ],
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "prepublishOnly": "pnpm build"
    },
    "dependencies": {
        "@pixi/core": "^7.4.3",
        "@pixi/display": "^7.4.3",
        "@pixi/ticker": "^7.4.3",
        "minimatch": "^10.0.1",
        "pixi-live2d-display-lipsyncpatch": "0.5.0-ls-7",
        "pixi.js": "^7.4.3"
    }
}
